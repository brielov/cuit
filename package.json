{
  "name": "cuit",
  "version": "1.2.0",
  "description": "Argentine CUIT utilities",
  "main": "dist/cuit.js",
  "module": "dist/cuit.mjs",
  "typings": "dist/cuit.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && rollup -c",
    "postinstall": "is-ci || husky install",
    "postpublish": "pinst --enable",
    "prepare": "npm run build",
    "prepublishOnly": "pinst --disable",
    "test": "jest"
  },
  "keywords": [
    "argentina",
    "cuit",
    "cuil",
    "validate"
  ],
  "license": "MIT",
  "author": "Gabriel Vaquer <brielov@icloud.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brielov/cuit.git"
  },
  "bugs": {
    "url": "https://github.com/brielov/cuit/issues"
  },
  "homepage": "https://github.com/brielov/cuit#readme",
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/node": "^16.10.2",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "esbuild": "^0.13.3",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.2",
    "is-ci": "^3.0.0",
    "jest": "^27.2.4",
    "lint-staged": "^11.1.2",
    "pinst": "^2.1.6",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.58.0",
    "rollup-plugin-dts": "^4.0.0",
    "rollup-plugin-esbuild": "^4.5.0",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.3"
  }
}
